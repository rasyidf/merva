{
	"$schema": "./node_modules/@biomejs/biome/configuration_schema.json",
	"organizeImports": {
	  "enabled": false
	},
	"linter": {
	  "enabled": true,
	  "rules": {
		"recommended": true,
		"suspicious": {
		  "noExplicitAny": "off"
		},
		"style": {
		  "all": true,
		  "useNamingConvention": "off",
		  "noImplicitBoolean": "off"
		}
	  }
	},
	"formatter": {
	  "enabled": true,
	  "indentWidth": 2,
	  "indentStyle": "space",
	  "lineWidth": 120
	}
  }
  